<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.serviceModel>
    <services>
      <service name="DeviceService.Service">
        <endpoint contract="DeviceService.IService"
                  binding="webHttpBinding"
                  address="http://localhost:5289/microsoft/device"
                  behaviorConfiguration="webHttp" >
        </endpoint>
      </service>
      <service name="CalculationService.Service">
        <endpoint contract="CalculationService.IService"
                  binding="webHttpBinding"
                  address="http://localhost:5289/microsoft/calculation"
                  behaviorConfiguration="webHttp" >
        </endpoint>
      </service>
    </services>
    <behaviors>>
      <endpointBehaviors>
        <behavior name="webHttp">
          <webHttp helpEnabled="true" faultExceptionEnabled="true" />
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>